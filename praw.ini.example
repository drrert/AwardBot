[<PRAW_SITE>]
# API keys from registering the bot on Reddit
client_id=<client-id>
client_secret=<client-secret>

# User authentication to perform actions
username=<username>
password=<password>

# Bot configuration and user-agent
bot_name=<bot-name>
bot_username=%(bot_name)s
bot_version=<bot-version>
bot_author=<bot-author>
user_agent=script:%(bot_name)s:v%(bot_version)s (by /u/%(bot_author)s)

# Character prefix used to ignore keywords on comments, e.g. \
bot_negate_char=<negate-char>
# Reset day: Monday == 0 ... Sunday == 6
bot_reset_day=<reset-day-number>
# Comma-separated list of users to ignore when checking keywords
bot_comments_blacklist=%(bot_username)s
